@model ProveedorTuTienditaPS4.Models.ViewModels.LoginViewModels
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Login - Tu Tiendita PS4</title>
    <link href="~/Content/Login/css/style.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="container">
            <h1>Welcome</h1>
            @using (Html.BeginForm("Login", "Home", FormMethod.Post, new { @class = "form", @id= "formLogin"}))
            {
                @Html.ValidationSummary(true)
                @Html.TextBoxFor(c => c.Username, new { @type = "text", @placeholder = "Username", @maxlength = "20" })
                @Html.TextBoxFor(c => c.Password, new { @type = "password", @placeholder = "Password", @maxlength = "20" })
                <input type="submit" value="Login" class="submint"/>
            }
        </div>
        <ul class="bg-bubbles">
            <li></li>
            <li></li>
            <li></li>
